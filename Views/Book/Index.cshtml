@model Bookish.Models.ListOfBooks;

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">

    <h1 class="display-4">Display a Book</h1>

    @foreach (var b in Model.BookList){
    <h3>Title: @b.Title</h3>
    <h3>Book ID: @b.BookId</h3>
    <h3>Author: @b.BookAuthor</h3>
    <h3>Published on: @b.Year</h3>
    }
    


</div>